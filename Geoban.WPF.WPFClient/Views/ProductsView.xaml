<Window x:Class="Geoban.WPF.WPFClient.Views.ProductsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Geoban.WPF.WPFClient.Views"
        xmlns:vm="clr-namespace:Geoban.WPF.WPFClient.ViewModels"
        mc:Ignorable="d"
        Title="ProductsView" Height="300" Width="300">

    <Window.DataContext>
        <vm:ProductsViewModel />
    </Window.DataContext>
    
    <Grid>

        
        <!-- ItemsControl -->
        
        <ListBox ItemsSource="{Binding Path=Products}"
                 >
        </ListBox>

    </Grid>
</Window>
